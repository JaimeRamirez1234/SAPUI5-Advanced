<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
<Table id="tb1" items="{odataNorthwind>Orders}" width="700px">
        <columns>
            <Column id="col1">
                <header>
                    <Label id="lb1" text="{i18n>orderID}"/>
                </header>
            </Column>
            <Column id="col2">
                <header>
                    <Label id="lb2" text="{i18n>freight}"/>
                </header>
            </Column>
            <Column id="col3">
                <header>
                    <Label id="lb3" text="{i18n>shipAddress}"/>
                </header>
            </Column>
        </columns>
        <items>
            <ColumnListItem id="cli1" type="Navigation" press="toOrderDetails">
                <cells>
                    <Label id="lbl1" text="{odataNorthwind>OrderID}"/>
                    <Label id="lbl2" text="{odataNorthwind>Freight}"/>
                    <Label id="lbl3" text="{odataNorthwind>ShipAddress}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>